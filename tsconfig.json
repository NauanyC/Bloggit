{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "./build/",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "lib": ["es6", "dom"],
    "noImplicitThis": true,
    "strictNullChecks": true
  },
  "include": ["src/index.tsx"]
}
